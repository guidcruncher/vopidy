 {
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": false,
    "verbatimModuleSyntax": false,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "baseUrl": ".",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "paths": {
      "@/*": [ "./src/*" ],
      "@/commands*": [ "./src/commands/*" ],
      "@/core": [ "./src/core/*" ],
      "@/middleware": [ "./src/middleware/*" ],
      "@/routes": [ "./src/routes/*" ],
      "@/rpc": [ "./src/rpc/*" ],
      "@/services": [ "./src/services/*" ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
